{
  "name": "Statim",
  "description": "Makes the first claim of the patent static in patents.google.com",
  "version": "1.4",
  "content_scripts": [
    {
      "matches": ["https://patents.google.com/patent/*"],
      "css": ["sheet.css"]
    }
  ],
  "manifest_version": 2,
    "page_action": {
      "default_icon": {
        "32": "icons/icon_statim32.png"
      }
    },
    "icons":{
      "128" : "icons/icon_statim128.png"
    }
}
